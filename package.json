{
  "name": "headstream",
  "version": "1.0.0",
  "description": "Landing page for Headstream",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "start": "npm run watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nischalNikit/HeadStream.git"
  },
  "author": "nischalNikit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nischalNikit/HeadStream/issues"
  },
  "homepage": "https://github.com/nischalNikit/HeadStream#readme",
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "node-sass": "^4.13.1",
    "npm-install-all": "^1.1.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.1"
  }
}
